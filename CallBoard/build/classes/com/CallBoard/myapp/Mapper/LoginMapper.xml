<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.CallBoard.myapp">
	<select id="getloginList" resultType="com.CallBoard.myapp.vo.loginVO">
		SELECT
		 	IDX
		,	USERNM
		,	USERPWD
		,	USEREMAIL
		,	REGDT  	 
		FROM 
			TB_ADMUSER
	</select>
	
</mapper>